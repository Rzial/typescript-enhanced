{
  "extends": ["eslint:recommended", "google"],

  "env": {
    "es6": true,
    "node": true
  },

  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "warn",
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "max-len": ["error", 121],
    "no-console": "warn"
  }
}
